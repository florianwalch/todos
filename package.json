{
  "name": "todos",
  "version": "1.0.0",
  "description": "Simple react todos app",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "rm -rf build && webpack serve --mode development",
    "storybook": "start-storybook -p 6006",
    "pretest": "rm -rf build/coverage-jest && rm -rf build/coverage && mkdir build/coverage",
    "test": "jest --coverage && cp build/coverage-jest/coverage-final.json build/coverage/coverage-final.json",
    "test-storybook": "test-storybook --coverage && cp coverage/storybook/coverage-storybook.json build/coverage/coverage-storybook.json",
    "posttest-storybook": "nyc report -t coverage/storybook --reporter=html --report-dir build/coverage-storybook && rm -rf coverage",
    "test-storybook:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server build/storybook --port 6006 --silent\" \"wait-on tcp:6006 && yarn test-storybook\"",
    "coverage-report": "nyc report -t build/coverage --reporter=html --report-dir build/coverage",
    "eslint": "eslint . --ext .js,.ts,.tsx",
    "prettier": "prettier -c ./**/*.{js,ts,tsx}",
    "check-types": "tsc --noEmit",
    "build-storybook": "build-storybook -o build/storybook",
    "prebuild": "rm -rf build && yarn check-types && yarn eslint && yarn build-storybook && yarn test && yarn test-storybook:ci && yarn coverage-report",
    "build": "webpack --mode production --color"
  },
  "dependencies": {
    "@aws-amplify/api": "4.0.53",
    "@aws-amplify/auth": "4.6.6",
    "@aws-amplify/cli": "10.0.0",
    "@aws-amplify/core": "4.7.4",
    "@aws-amplify/ui": "3.13.3",
    "@aws-amplify/ui-react": "3.5.2",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.3",
    "@mui/material": "5.10.5",
    "aws-amplify": "4.3.35",
    "dayjs": "1.11.5",
    "graphql-tag": "2.12.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.35.0",
    "react-router-dom": "6.4.0",
    "react-transition-group": "4.4.5",
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.19.1",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/addon-coverage": "0.0.4",
    "@storybook/addon-storyshots": "6.5.12",
    "@storybook/jest": "0.0.10",
    "@storybook/builder-webpack5": "6.5.12",
    "@storybook/manager-webpack5": "6.5.12",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@storybook/test-runner": "0.7.2",
    "@testing-library/react": "13.4.0",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "@types/jest": "28.1.1",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "babel-loader": "8.2.5",
    "babel-plugin-direct-import": "1.0.0",
    "css-loader": "6.7.1",
    "mini-css-extract-plugin": "2.6.1",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.6.4",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "prettier": "2.7.1",
    "ts-jest": "29.0.1",
    "ts-loader": "9.3.1",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "terser-webpack-plugin": "5.3.6",
    "unused-webpack-plugin": "2.4.0",
    "copy-webpack-plugin": "11.0.0",
    "html-webpack-plugin": "5.5.0",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0",
    "webpack-bundle-analyzer": "4.6.1",
    "webpack-dev-server": "4.11.0",
    "concurrently": "7.4.0"
  }
}
